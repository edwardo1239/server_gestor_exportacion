{
  "source": {
    "include": ["index.js", "DB"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "opts": {
    "destination": "./docs",
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
